<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>dark</theme>

		<!-- viewstyle -->
		<!-- alt-view -->
		<viewstyle>alt-view</viewstyle>

		<!-- converts nodes into toggles -->
		<!-- true, false -->
		<nodesAsToggles>true</nodesAsToggles>

		<!-- converts nodes into accordion -->
		<!-- true, false -->
		<nodesAsAccordion>true</nodesAsAccordion>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- offline report -->
		<enableOfflineMode>false</enableOfflineMode>

		<!-- enable or disable timeline on dashboard -->
		<enableTimeline>true</enableTimeline>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>

		<!-- title of the document -->
		<documentTitle>1Huddle Automation Report</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>
		1huddle 
		<![CDATA[
            <img src='./logo.png' />
        ]]>  
		
		</reportName>

		<!-- timestamp format -->
		<timeStampFormat>MM-dd-yyyy, HH:mm:ss</timeStampFormat>

		<!-- custom javascript -->
		
        <scripts>
            <![CDATA[
                $(document).ready(function() {
               $('.page-container.main-content.test-list.test-list-tools.tools pull-left.font-size-14:first-child').text('Tests View- Changed Heading');
});
            ]]>
        </scripts>
        
        
        

		<!-- custom styles -->
		<styles>
        <![CDATA[
            .reportName { padding-left: 1000px; } .report-name > img { float: 
      left;height: 190%;margin-left: 1000px;margin-top: 5px;width: auto; }
        ]]>
    </styles>
		
		<!-- custom styles -->
<css>
    <![CDATA[
        <!-- I've put my styles here -->
     ]]>
</css>`
		
	</configuration>
</extentreports>